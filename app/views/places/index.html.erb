<% @places.each do |place| %>
<div class="booyah-box col-10 offset-1">
  <h1><%= place.name %></h1><br />
  <i><%= place.address %></i><br /><br />
  <p><%= place.description %></p><br />
</div>
<% end %>
<br class="clear" />
<div class="container">

<div class="col-5 offset-1 pagination a pagination a ">
<%= will_paginate @places %>
</div>
</div>

